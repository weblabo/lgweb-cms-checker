{
   "manifest_version": 3,
   "name": "自治体公式サイト CMS Checker",
   "version": "0.1",
   "description": "自治体公式サイトで使われているCMSをチェックするアドオン",
   "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "cms_checker_popup.html"
    },
   "icons": {
     "16": "icons/icon16.png",
     "48": "icons/icon48.png",
     "128": "icons/icon128.png"
   }
 }